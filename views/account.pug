extends layout.pug

block content
  h1 Account info
  img(src=user.photo style='width:150px; border-radius:25%')
  h3= user.username
  p Login provider: #{user.provider.toUpperCase()}
  p Tracker Account created on: #{user.created_on.toLocaleString()}
  p Last Login: #{user.last_login.toLocaleString()}
  p Number of Logins: #{user.login_count}
